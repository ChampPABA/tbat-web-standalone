    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/nodemailer": "^7.0.1",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "dompurify": "^3.2.6",
    "express-rate-limit": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.542.0",
    "next": "^15.5.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "playwright": "^1.43.0",
    "postcss": "^8.5.6",
    "prisma": "^6.15.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "redis": "^5.8.2",
    "resend": "^6.0.2",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "stripe": "^18.5.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.5"
