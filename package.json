{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:auto": "node scripts/start-with-port-detection.js",
    "dev:smart": "node scripts/start-with-port-detection.js",
    "dev:3001": "next dev --port 3001",
    "dev:3002": "next dev --port 3002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 50",
    "check-types": "tsc --noEmit",
    "playwright:setup": "node scripts/detect-dev-port.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest",
    "test:integration": "jest --testPathPattern=__tests__.*integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:db": "jest --testPathPattern=prisma",
    "test:auth": "jest --testPathPattern=auth",
    "test:payments": "jest --testPathPattern=stripe",
    "test:email": "jest --testPathPattern=email",
    "test:cache": "jest --testPathPattern=redis",
    "test:migration-rollback": "node scripts/test-migration-rollback.mjs",
    "db:rollback-test": "npm run test:migration-rollback",
    "db:monitor": "node -e \"console.log('Database monitoring available at /api/health/database')\"",
    "db:health-check": "curl -s http://localhost:3000/api/health/database?metrics=true&details=true | json_pp"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
